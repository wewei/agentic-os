{
  "name": "@agentic-os/cli",
  "version": "0.1.0",
  "description": "Agentic OS CLI - Terminal UI for interacting with agents",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "type": "module",
  "bin": {
    "agentic": "./src/cli.ts"
  },
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    }
  },
  "files": [
    "src",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "bun run lint",
    "dev": "bun --watch src/cli.ts",
    "start": "bun run src/cli.ts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@agentic-os/core": "workspace:*",
    "blessed": "^0.1.81",
    "js-yaml": "^4.1.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/blessed": "^0.1.25",
    "@types/bun": "latest",
    "@types/eslint__js": "^8.42.3",
    "@types/js-yaml": "^4.0.9",
    "eslint": "^9.15.0",
    "eslint-plugin-import": "^2.31.0",
    "typescript-eslint": "^8.15.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "keywords": [
    "agent",
    "ai",
    "cli",
    "tui",
    "terminal",
    "blessed",
    "typescript",
    "bun"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wewei/agentic-os.git",
    "directory": "packages/cli"
  }
}

